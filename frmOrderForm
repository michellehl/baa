    Private Sub cboItemType_SelectedIndexChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles cboItemType.SelectedIndexChanged
        Try
            'When an item is selected from the combobox cboItemType it clears the dataset dsItem, 
            'which is used for the combobox cboitem, opens the daItem connection 
            'fills the where value for the daItem command from the combobox cboItemtype
            'creates a datareader then executes the reader and them closes the connection. 
            'then fills the dataset dsItem fro mthe daItem
            DsItem.Clear()
            daItem.SelectCommand.Connection.Open()
            daItem.SelectCommand.Parameters("itemtype").Value = ToInt32(Me.cboItemType.Text)
            Dim rdrOrderID As OleDb.OleDbDataReader

            rdrOrderID = daOrderID.SelectCommand.ExecuteReader()
            rdrOrderID.Read()
            daItem.SelectCommand.Connection.Close()

            daItem.Fill(DsItem)

        Catch sqlEX As SqlClient.SqlException
            'This will catch sql exceptions
            MessageBox.Show(sqlEX.Message)

        Catch ex As Exception
            'This will catch all others
            MessageBox.Show(ex.Message)
        End Try

    End Sub

    Private Sub btnSend_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnSend.Click
        Try
            Dim intitemtype As Integer
            Dim intitem As Integer
            Dim intcount As Integer
            If txtReciept.Text = "" Then
                MsgBox("You must enter the receipt number located on the right hand corner of the ticket order")
                txtReciept.Focus()
                Exit Sub
            End If
            If Me.txtAmt.Text = "" Then
                MsgBox("You must enter the price of the item to continue.")
                txtAmt.Focus()
                Exit Sub
            End If

            Dim dblarttotal As Double
            If txtnotes.Text = "" Then
                intitemtype = ToInt32(Me.cboItemType.Text)
                intitem = ToInt32(Me.cboItem.Text)






                Dim cnTag1 As New OleDb.OleDbConnection
                cnTag1.ConnectionString = "Provider=Microsoft.Jet.OLEDB.4.0;Password=;User ID=Admin;data source=c:\BAA.mdb"

                Dim sqlTag1 As String = "select * from tblitem where itemtype = ? and itemid = ? "
                Dim cmTag1 As New OleDb.OleDbCommand(sqlTag1, cnTag1)
                cmTag1.Parameters.Add(New OleDb.OleDbParameter("itemtype", OleDb.OleDbType.Integer, 0))
                cmTag1.Parameters.Add(New OleDb.OleDbParameter("itemid", OleDb.OleDbType.Integer, 0))
                cmTag1.Parameters("itemtype").Value = intitemtype
                cmTag1.Parameters("itemid").Value = intitem
                cnTag1.Open()
                Dim rdrtotal As OleDb.OleDbDataReader
                rdrtotal = cmTag1.ExecuteReader()
                Do While rdrtotal.Read()
                    intcount = rdrtotal.Item("itemcount")

                Loop
                rdrtotal.Close()
                cnTag1.Close()

                If intcount <= 0 Then
                    MsgBox("There is 0 items in the inventory for item " + Me.lblItemDescript.Text + " with an itemtype " + Me.cboItemType.Text + " Please check your order.")
                    Exit Sub
                End If
            End If


            If Me.txtnotes.Text = "Art Only" Then

                daInsertTransaction.InsertCommand.Connection.Open()
                daInsertTransaction.InsertCommand.Parameters("orderid").Value = ToInt32(Me.lblOrderID.Text)
                daInsertTransaction.InsertCommand.Parameters("itemid").Value = 0
                daInsertTransaction.InsertCommand.Parameters("itemdesc").Value = ""
                daInsertTransaction.InsertCommand.Parameters("itemcost").Value = 0
                daInsertTransaction.InsertCommand.Parameters("arttotal").Value = ToDouble(Me.txtAmt.Text)
                daInsertTransaction.InsertCommand.Parameters("subTotal").Value = ToDouble(Me.txtAmt.Text)
                daInsertTransaction.InsertCommand.Parameters("notes").Value = Me.txtnotes.Text
                daInsertTransaction.InsertCommand.ExecuteNonQuery()
                daInsertTransaction.InsertCommand.Connection.Close()

            ElseIf Me.txtnotes.Text <> "Art Only" Then

                Dim dblamt As Double = ToDouble(Me.txtAmt.Text)
                Dim dblitemamt As Double = ToDouble(Me.txtItemAmt.Text)
                dblarttotal = dblamt - dblitemamt


                daInsertTransaction.InsertCommand.Connection.Open()
                daInsertTransaction.InsertCommand.Parameters("orderid").Value = ToInt32(Me.lblOrderID.Text)
                daInsertTransaction.InsertCommand.Parameters("itemid").Value = ToInt32(Me.cboItem.Text)
                daInsertTransaction.InsertCommand.Parameters("itemdesc").Value = Me.lblItemDescript.Text
                daInsertTransaction.InsertCommand.Parameters("itemcost").Value = ToDouble(Me.txtItemAmt.Text)
                daInsertTransaction.InsertCommand.Parameters("arttotal").Value = dblarttotal
                daInsertTransaction.InsertCommand.Parameters("subTotal").Value = ToDouble(Me.txtAmt.Text)
                daInsertTransaction.InsertCommand.Parameters("notes").Value = ""
                daInsertTransaction.InsertCommand.ExecuteNonQuery()
                daInsertTransaction.InsertCommand.Connection.Close()
            End If





            Call GetTransactions()

            Call GetTotals()




        Catch sqlEX As SqlClient.SqlException
            'This will catch sql exceptions
            MessageBox.Show(sqlEX.Message)

        Catch ex As Exception
            'This will catch all others
            MessageBox.Show(ex.Message)
        End Try


    End Sub
    Private Sub GetTotals()

        Dim dblsubtotal As Double
        Dim dbltax As Double
        Dim intcount As Integer = 0
        Me.daTransactions.SelectCommand.Connection.Open()
        daTransactions.SelectCommand.Parameters("orderid").Value = ToInt32(Me.lblOrderID.Text)
        Dim rdrtotal As OleDb.OleDbDataReader
        rdrtotal = daTransactions.SelectCommand.ExecuteReader()
        Do While rdrtotal.Read()
            intcount += 1

        Loop
        rdrtotal.Close()
        daTransactions.SelectCommand.Connection.Close()
        If intcount > 0 Then
            daTotals.SelectCommand.Connection.Open()
            daTotals.SelectCommand.Parameters("orderid").Value = ToInt32(Me.lblOrderID.Text)
            Dim rdrtotals As OleDb.OleDbDataReader
            rdrtotals = daTotals.SelectCommand.ExecuteReader()
            Do While rdrtotals.Read()
                dblsubtotal = rdrtotals.Item("stot")
                mdblarttotal = rdrtotals.Item("art")
            Loop
            rdrtotals.Close()
            daTotals.SelectCommand.Connection.Close()
            daTax.SelectCommand.Connection.Open()
            Dim rdrtax As OleDb.OleDbDataReader
            rdrtax = daTax.SelectCommand.ExecuteReader

            Do While rdrtax.Read
                dbltax = rdrtax.Item("statetax")
            Loop
            rdrtax.Close()
            daTax.SelectCommand.Connection.Close()

            dbltax = dbltax * dblsubtotal
            dbltax = Math.Round(dbltax, 2)

            Me.lblSubtotal.Text = dblsubtotal
            Me.lblTax.Text = dbltax
            Me.lblTotal.Text = dblsubtotal + dbltax
        End If



    End Sub

    Private Sub btnClear_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnClear.Click
        Try
           
            

            Dim cnShirt As New OleDb.OleDbConnection
            cnShirt.ConnectionString = "Provider=Microsoft.Jet.OLEDB.4.0;Password=;User ID=Admin;data source=c:\BAA.mdb"
            Dim sqlShirt As String = "Delete * from TBLTRAN where orderID = ?"
            Dim cmShirt As New OleDb.OleDbCommand(sqlShirt, cnShirt)
            cmShirt.Parameters.Add(New OleDb.OleDbParameter("orderid", OleDb.OleDbType.Integer, 0))
            cmShirt.Parameters("orderid").Value = ToInt32(lblOrderID.Text)
            cnShirt.Open()
            cmShirt.ExecuteNonQuery()
            cnShirt.Close()

            Call ClearOrder()
            Call GetTransactions()

        Catch sqlEX As SqlClient.SqlException
            'This will catch sql exceptions
            MessageBox.Show(sqlEX.Message)

        Catch ex As Exception
            'This will catch all others
            MessageBox.Show(ex.Message)
        End Try
    End Sub


    Private Sub btnArtOnly_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnArtOnly.Click

        If mblnNotes = True Then
            Me.txtnotes.Text = "Art Only"

            Call notes()
        Else
            Me.txtnotes.Text = ""
            mblnNotes = True

        End If


    End Sub
    Private Sub notes()
        mblnNotes = False
    End Sub

    Private Sub btnSubtotal_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnSubtotal.Click
        If mstrSubtotal <> "" Then
            Me.txtAmtGiven.Text = mstrSubtotal
        End If
        If Me.txtAmtGiven.Text = "" Then
            Me.lblChange.Text = ""
        Else
            lblChange.Text = Math.Round((ToDouble(lblTotal.Text) - ToDouble(txtAmtGiven.Text)) * -1, 2)

        End If
        If lblChange.Text = "" Then
        Else
            If ToDouble(lblChange.Text) < 0 Then
                lblChange.ForeColor = ForeColor.Red
            Else
                lblChange.ForeColor = ForeColor.Black

            End If
        End If
        mstrSubtotal = ""
    End Sub
    Private Sub INVENTORYUPDATE()
        Try
            Dim intItemID As Integer
            Dim strdescr As String
            Dim intCount As Integer
            Dim intC As Integer
            Dim inttran As Integer
            Dim intt As Integer
            Dim cnCount As New OleDb.OleDbConnection
            cnCount.ConnectionString = "Provider=Microsoft.Jet.OLEDB.4.0;Password=;User ID=Admin;data source=c:\BAA.mdb"
            Dim sqlCount As String = "Select * from tbltran where orderid = ?"
            Dim cmCount As New OleDb.OleDbCommand(sqlCount, cnCount)
            cmCount.Parameters.Add(New OleDb.OleDbParameter("orderid", OleDb.OleDbType.Integer, 0))
            cmCount.Parameters("orderid").Value = ToInt32(Me.lblOrderID.Text)
            cnCount.Open()
            Dim rdrCount As OleDb.OleDbDataReader
            rdrCount = cmCount.ExecuteReader
            Do While rdrCount.Read
                intCount += 1

            Loop
            rdrCount.Close()
            cnCount.Close()
            intCount -= 1
            inttran = intCount
            ReDim Preserve intTranid(inttran)

            cnCount.Open()

            rdrCount = cmCount.ExecuteReader
            Do While rdrCount.Read
                intTranid(intC) = rdrCount.Item("tranid")

                intC += 1

            Loop
            rdrCount.Close()
            cnCount.Close()
            For intC = 0 To intCount


                Dim cnShirt As New OleDb.OleDbConnection
                cnShirt.ConnectionString = "Provider=Microsoft.Jet.OLEDB.4.0;Password=;User ID=Admin;data source=c:\BAA.mdb"
                Dim sqlShirt As String = "Select * from tbltran where orderid = ? and tranid = ?"
                Dim cmShirt As New OleDb.OleDbCommand(sqlShirt, cnShirt)
                cmShirt.Parameters.Add(New OleDb.OleDbParameter("orderid", OleDb.OleDbType.Integer, 0))
                cmShirt.Parameters.Add(New OleDb.OleDbParameter("tranid", OleDb.OleDbType.Integer, 0))
                cmShirt.Parameters("tranid").Value = intTranid(intC)
                cmShirt.Parameters("orderid").Value = ToInt32(Me.lblOrderID.Text)
                cnShirt.Open()
                Dim rdrShirt As OleDb.OleDbDataReader
                rdrShirt = cmShirt.ExecuteReader
                Do While rdrShirt.Read
                    intItemID = rdrShirt.Item("itemid")
                    strdescr = rdrShirt.Item("itemdesc")

                Loop
                rdrShirt.Close()


                cnShirt.Close()
                Dim cnTag1 As New OleDb.OleDbConnection
                cnTag1.ConnectionString = "Provider=Microsoft.Jet.OLEDB.4.0;Password=;User ID=Admin;data source=c:\BAA.mdb"

                Dim sqlTag1 As String = "Update tblitem set itemcount = itemcount - 1 where itemid = ? "
                Dim cmTag1 As New OleDb.OleDbCommand(sqlTag1, cnTag1)
                cmTag1.Parameters.Add(New OleDb.OleDbParameter("itemid", OleDb.OleDbType.Integer, 0))
                cmTag1.Parameters("itemid").Value = intItemID
                cnTag1.Open()
                cmTag1.ExecuteNonQuery()

                cnTag1.Close()

                Me.DsItem.Clear()
                Me.daItem.Fill(DsItem)

            Next
        Catch sqlEX As SqlClient.SqlException
            'This will catch sql exceptions
            MessageBox.Show(sqlEX.Message)

        Catch ex As Exception
            'This will catch all others
            MessageBox.Show(ex.Message)
        End Try
    End Sub
    Private Sub btnOrderComplete_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnOrderComplete.Click
        Try
            If txtnotes.Text = "" Then
                Call INVENTORYUPDATE()
            End If

            Dim strretailloc As String = Me.lblRetailStore.Text
            Dim intRSid As Integer


            Dim strOrderID As String
            Dim strRetail As String
            Dim INTORDERID As Integer = ToInt32(lblOrderID.Text)
            Dim intinvoice As Integer = ToInt32(Me.txtReciept.Text)
            Dim strArtist As String
            If cboArtist.Visible = True Then
                strArtist = cboArtist.Text
            Else
                strArtist = "Unknown"
            End If



            daInsertOrder.InsertCommand.Connection.Open()
            daInsertOrder.InsertCommand.Parameters("orderid").Value = ToInt32(Me.lblOrderID.Text)
            daInsertOrder.InsertCommand.Parameters("invoiceid").Value = ToInt32(Me.txtReciept.Text)
            daInsertOrder.InsertCommand.Parameters("storeid").Value = ToInt32(Mid(Me.lblOrderID.Text, 1, 1))
            daInsertOrder.InsertCommand.Parameters("Artist").Value = strArtist
            daInsertOrder.InsertCommand.Parameters("SubTotal").Value = ToDouble(Me.lblSubtotal.Text)
            daInsertOrder.InsertCommand.Parameters("arttotal").Value = mdblarttotal
            daInsertOrder.InsertCommand.Parameters("Tax").Value = ToDouble(Me.lblTax.Text)
            daInsertOrder.InsertCommand.Parameters("Total").Value = ToDouble(Me.lblTotal.Text)
            daInsertOrder.InsertCommand.Parameters("trandate").Value = Me.dtdate.Text
            daInsertOrder.InsertCommand.Parameters("sales").Value = Me.cboSales.Text

            daInsertOrder.InsertCommand.ExecuteNonQuery()
            daInsertOrder.InsertCommand.Connection.Close()



            Dim sqlShirt As String = "Select * from TBLRETAIL where RETAILNAME = ?"
            Dim cmShirt As New OleDb.OleDbCommand(sqlShirt, cnBAA)
            cmShirt.Parameters.Add(New OleDb.OleDbParameter("RETAILNAME", OleDb.OleDbType.VarChar, 50))
            cmShirt.Parameters("RETAILNAME").Value = strretailloc
            cnBAA.Open()
            Dim rdrShirt As OleDb.OleDbDataReader
            rdrShirt = cmShirt.ExecuteReader
            Do While rdrShirt.Read
                intRSid = rdrShirt.Item("retailid")

            Loop
            rdrShirt.Close()
            cnBAA.Close()

            daOrderID.SelectCommand.Connection.Open()
            daOrderID.SelectCommand.Parameters("storeid").Value = intRSid
            Dim rdrOrderID As OleDb.OleDbDataReader

            rdrOrderID = daOrderID.SelectCommand.ExecuteReader()
            rdrOrderID.Read()

            INTORDERID = rdrOrderID.Item("LastID")
            strOrderID = INTORDERID.ToString
            strRetail = Mid(strOrderID, 1, 1)
            strOrderID = Mid(strOrderID, 2)
            INTORDERID = ToInt32(strOrderID)
            INTORDERID += 1
            strOrderID = strRetail + INTORDERID.ToString
            lblOrderID.Text = strOrderID
            rdrOrderID.Close()
            daOrderID.SelectCommand.Connection.Close()

            DsTransactions.Clear()


            Call ClearOrder()


        Catch sqlEX As SqlClient.SqlException
            'This will catch sql exceptions
            MessageBox.Show(sqlEX.Message)

        Catch ex As Exception
            'This will catch all others
            MessageBox.Show(ex.Message)
        End Try
    End Sub
    Private Sub ClearOrder()
        txtnotes.Text = ""
        txtAmt.Text = ""
        txtReciept.Text = ""
        lblSubtotal.Text = ""
        lblTax.Text = ""
        lblTotal.Text = ""
        mdblarttotal = 0
        txtAmtGiven.Text = ""
        lblChange.Text = ""


    End Sub










    
   
End Class
