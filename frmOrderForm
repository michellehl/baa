    'declare arrays
    Public intTranid() As Integer
    Private Sub frmOrderForm_Load(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles MyBase.Load
        dtdate.Value = DateTime.Now
        Dim intRetailValid As Integer

        Dim strretailloc As String = Me.lblRetailStore.Text
        Dim intRSid As Integer

        Dim intOrderID As Integer
        Dim strOrderID As String
        Dim strRetail As String
        daSales.Fill(DsSales)
        daArtist.Fill(DsArtist)
        daItemType.Fill(DsItemType)



        Dim sqlShirt As String = "Select * from TBLRETAIL where RETAILNAME = ? and status = 'A'"
        Dim cmShirt As New OleDb.OleDbCommand(sqlShirt, cnBAA)
        cmShirt.Parameters.Add(New OleDb.OleDbParameter("RETAILNAME", OleDb.OleDbType.VarChar, 50))
        cmShirt.Parameters("RETAILNAME").Value = strretailloc
        cnBAA.Open()
        Dim rdrShirt As OleDb.OleDbDataReader
        rdrShirt = cmShirt.ExecuteReader
        Do While rdrShirt.Read
            intRSid = rdrShirt.Item("retailid")

        Loop
        rdrShirt.Close()
        cnBAA.Close()
        If intRSid = 0 Then
            MsgBox("You must have a valid store to continue with order. Exit Form")
        End If
        Dim sqlValidStore As String = "select * from tblorder where storeid = ?"
        Dim cmValidStore As New OleDb.OleDbCommand(sqlValidStore, cnBAA)
        cmValidStore.Parameters.Add(New OleDb.OleDbParameter("storeid", OleDb.OleDbType.Integer, 0))
        cmValidStore.Parameters("storeid").Value = intRSid
        cnBAA.Open()
        Dim rdrValidStore As OleDb.OleDbDataReader
        rdrValidStore = cmValidStore.ExecuteReader
        Do While rdrValidStore.Read
            intRetailValid += 1
        Loop
        rdrValidStore.Close()
        cnBAA.Close()

        If intRetailValid = 0 Then
            MsgBox("You must have a valid store to continue with order.Exit Form")
            Exit Sub
        End If
        daOrderID.SelectCommand.Connection.Open()
        daOrderID.SelectCommand.Parameters("storeid").Value = intRSid
        Dim rdrOrderID As OleDb.OleDbDataReader

        rdrOrderID = daOrderID.SelectCommand.ExecuteReader()
        rdrOrderID.Read()

        intOrderID = rdrOrderID.Item("LastID")
        strOrderID = intOrderID.ToString
        strRetail = Mid(strOrderID, 1, 1)
        strOrderID = Mid(strOrderID, 2)
        intOrderID = ToInt32(strOrderID)
        intOrderID += 1
        strOrderID = strRetail + intOrderID.ToString
        lblOrderID.Text = strOrderID
        rdrOrderID.Close()
        daOrderID.SelectCommand.Connection.Close()
        Call GetTransactions()
        Call GetTotals()
        Call FillItems()


    End Sub
    Private Sub FillItems()
        Try
            Dim intone As Integer = 1
            'which is used for the combobox cboitem, opens the daItem connection 
            'fills the where value for the daItem command from the combobox cboItemtype
            'creates a datareader then executes the reader and them closes the connection. 
            'then fills the dataset dsItem fro mthe daItem
            DsItem.Clear()
            daItem.SelectCommand.Connection.Open()
            daItem.SelectCommand.Parameters("itemtype").Value = ToInt32(intone)
            Dim rdrOrderID As OleDb.OleDbDataReader

            rdrOrderID = daOrderID.SelectCommand.ExecuteReader()
            rdrOrderID.Read()
            daItem.SelectCommand.Connection.Close()

            daItem.Fill(DsItem)

        Catch sqlEX As SqlClient.SqlException
            'This will catch sql exceptions
            MessageBox.Show(sqlEX.Message)

        Catch ex As Exception
            'This will catch all others
            MessageBox.Show(ex.Message)
        End Try
    End Sub
    Private Sub GetTransactions()
        DsTransactions.Clear()
        daTransactions.SelectCommand.Connection.Open()
        daTransactions.SelectCommand.Parameters("orderid").Value = ToInt32(Me.lblOrderID.Text)

        daTransactions.SelectCommand.Connection.Close()
        daTransactions.Fill(DsTransactions)
    End Sub

    Private Sub chkArtist_CheckedChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles chkArtist.CheckedChanged
        If chkArtist.CheckState = CheckState.Checked Then
            Me.cboArtist.Visible = True
        ElseIf chkArtist.CheckState = CheckState.Unchecked Then
            Me.cboArtist.Visible = False
        End If
    End Sub
